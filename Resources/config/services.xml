<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="symfonians_svp.svp_extension.class">Symfonians\SVPBundle\Twig\SVPExtension</parameter>
    </parameters>

    <services>
        <service id="symfonians_svp.svp_extension" class="%symfonians_svp.svp_extension.class%">
            <tag name="twig.extension" />
        </service>
    </services>

</container>
